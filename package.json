{
  "name": "svelte-app-template",
  "version": "0.1.3",
  "description": "Svelte app template",
  "main": "rollup.config.js",
  "author": "ink8bit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ink8bit/svelte-app-template.git"
  },
  "bugs": {
    "url": "https://github.com/ink8bit/svelte-app-template/issues"
  },
  "homepage": "https://github.com/ink8bit/svelte-app-template#readme",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "check": "svelte-check",
    "type:check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts",
    "format:check": "prettier --check \"src/**/*\"",
    "format": "prettier --write \"src/**/*\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.40.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.3",
    "svelte-preprocess": "^4.6.9",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
